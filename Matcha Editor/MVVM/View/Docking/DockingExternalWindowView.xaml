<Window x:Class="Matcha_Editor.MVVM.View.DockingExternalWindowView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Matcha_Editor.MVVM.View"
        mc:Ignorable="d"
        WindowStyle="None"
        Background="{StaticResource Editor.Window.GrayBrush1}"
        Title="DockingExternalWindowView" Height="450" Width="300">
    <WindowChrome.WindowChrome>
        <WindowChrome CaptionHeight="0"/>
    </WindowChrome.WindowChrome>
    <DockPanel>
        <Border DockPanel.Dock="Top" Background="{StaticResource Editor.Window.GrayBrush1}" Margin="0,0,0,-20" Height="20" MouseDown="Border_MouseDown"></Border>
        <local:DockingContainerView x:Name="DockingContainer"/>
    </DockPanel>
</Window>
