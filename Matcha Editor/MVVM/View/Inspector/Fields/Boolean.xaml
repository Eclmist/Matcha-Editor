<UserControl x:Class="Matcha_Editor.MVVM.View.Boolean"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Matcha_Editor.MVVM.View"
             mc:Ignorable="d" 
             d:DesignHeight="26" d:DesignWidth="400">
    <Grid>
        <CheckBox IsChecked="{Binding Mode=TwoWay, Path=Field.Enabled}"></CheckBox>
    </Grid>
</UserControl>
